{
  "manifest_version": 2,

  "name": "Special Character to utf-16 converter",
  "version": "0.1.0",
  "description": "Special Character to utf-16 converter",
  "icons": {
    "16": "image48.png",
    "48": "image48.png",
    "128": "image48.png"
  },

  "content_scripts": [
    {
      "css": [],
      "run_at": "document_end",
      "js": ["jquery-3.3.1.slim.min.js"],
      "matches": ["*://*/*"]
    }
  ],

  "options_page": "settings.html",
  "permissions": ["storage"]
}
